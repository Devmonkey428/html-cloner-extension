{
    "manifest_version": 3,
    "name": "Html5 Game Cloner",
    "version": "1.0",
    "description": "This is chrome extension that clones html5 games.",
    "permissions": [
      "webRequest",
      "storage",
      "downloads",
      "tabs",
      "notifications"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  